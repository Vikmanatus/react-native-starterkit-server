{
  "path-intellisense.mappings": {
    "@": "${workspaceRoot}/src"
  },
  "editor.formatOnSave": true,
  "prettier.requireConfig": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript", "typescript"],
  "files.associations": {
    "yarn.lock": "text",
    "package.json": "jsonc"
  }
}
